{
  "name": "au2-data-grid",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/types/index.d.ts",
  "type": "module",
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "npm run clean",
    "build:ts": "tsc",
    "build:other": "cpx ./src/**/*.{html,css} ./dist",
    "build": "run-p build:ts build:other",
    "watch:ts": "tsc --watch --preserveWatchOutput",
    "watch:other": "cpx ./src/**/*.{html,css} ./dist --watch",
    "watch": "run-p watch:ts watch:other"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "cpx2": "^4.1.1",
    "rimraf": "^3.0.2",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "@aurelia/kernel": "latest",
    "@aurelia/runtime": "latest",
    "@aurelia/runtime-html": "latest"
  }
}